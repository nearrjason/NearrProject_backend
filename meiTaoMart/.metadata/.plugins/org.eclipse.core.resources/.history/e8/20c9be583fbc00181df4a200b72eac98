package com.meitaomart.order.pojo;

import java.io.Serializable;
import java.util.List;

import com.meitaomart.pojo.MeitaoOrder;
import com.meitaomart.pojo.TbOrder;
import com.meitaomart.pojo.TbOrderItem;
import com.meitaomart.pojo.TbOrderShipping;

public class OrderInfo extends MeitaoOrder implements Serializable {

	private List<TbOrderItem> orderItems;
	private MeitaoOrderShipping orderShipping;
	public List<TbOrderItem> getOrderItems() {
		return orderItems;
	}
	public void setOrderItems(List<TbOrderItem> orderItems) {
		this.orderItems = orderItems;
	}
	public TbOrderShipping getOrderShipping() {
		return orderShipping;
	}
	public void setOrderShipping(TbOrderShipping orderShipping) {
		this.orderShipping = orderShipping;
	}
	
}
